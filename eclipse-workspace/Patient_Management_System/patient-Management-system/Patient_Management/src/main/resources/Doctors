DROP TABLE Management IF EXISTS;

CREATE TABLE Doctors (
id Integer,
firstName VARCHAR(50),
lastName VARCHAR(50),
email VARCHAR(20),
password VARCHAR(50),
sex VARCHAR(20),
specialization VARCHAR(20)
 );

